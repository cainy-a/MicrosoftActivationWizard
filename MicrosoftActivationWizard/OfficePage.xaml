<Page x:Class="MicrosoftActivationWizard.OfficePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MicrosoftActivationWizard"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="OfficePage">

    <StackPanel HorizontalAlignment="Center">
        <TextBlock Margin="5" FontSize="20" TextWrapping="Wrap" TextAlignment="Justify">Do you have any products other than Office 2019 (Visio, Project, Office 2013, etc)?</TextBlock>

        <Button Name="ButtonOfficeNonStandard" Click="ButtonOfficeNonStandard_OnClick" Margin="5" Height="25">Yes</Button>
        <Button Name="ButtonOfficeStandard" Click="ButtonOfficeStandard_OnClick" Margin="5" Height="25">No</Button>
    </StackPanel>
</Page>
